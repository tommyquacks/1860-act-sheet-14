// Sys.vm
function Sys.init 0
    return

function Sys.min 1
    push argument 0
    push argument 1
    pop local 0
    sub
    push local 0
    if-goto NEG
    push local 0
    return
label NEG
    push argument 0
    return

function Sys.max 1
    push argument 0
    push argument 1
    pop local 0
    sub
    push local 0
    if-goto POS
    push local 0
    return
label POS
    push argument 0
    return

function Sys.range 4
    push argument 1
    push constant 1
    sub
    if-goto LE1
    push argument 0
    pop pointer 1
    push that 0
    pop local 0
    push that 0
    pop local 1
    push constant 1
    pop local 2
    push argument 1
    pop local 3
label LOOP
    push local 2
    push local 3
    sub
    if-goto END
    push argument 0
    push local 2
    add
    pop pointer 1
    push local 0
    push that 0
    call Sys.min 2
    pop local 0
    push local 1
    push that 0
    call Sys.max 2
    pop local 1
    push local 2
    push constant 1
    add
    pop local 2
    goto LOOP
label END
    push local 1
    push local 0
    sub
    return
label LE1
    push constant 0
    return
